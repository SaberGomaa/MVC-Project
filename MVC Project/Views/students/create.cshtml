@model MVC_Project.Models.student
@{
    ViewBag.Title = "create";
}

@*<script src="~/Scripts/jquery-3.4.1.min.js"></script>
<script src="~/Scripts/jquery.validate.js"></script>
<script src="~/Scripts/jquery.validate.unobtrusive.js"></script>
<script src="~/Scripts/jquery-1.8.0.js"></script>*@

<form method="post" enctype="multipart/form-data">
    <table>
        <tr>
            <td>@Html.LabelFor(e => e.name)</td>
            <td>@Html.EditorFor(e => e.name)</td>
            <td>@Html.ValidationMessageFor(e => e.name)</td>
        </tr>
        <tr>
            <td>age</td>
            <td>@Html.EditorFor(e => e.age)</td>
            <td>@Html.ValidationMessageFor(e => e.age)</td>

        </tr>
        <tr>
            <td>address</td>
            <td>@Html.EditorFor(e => e.address)</td>
            <td>@Html.ValidationMessageFor(e => e.address)</td>

        </tr>
        <tr>
            <td>Email</td>
            <td>@Html.EditorFor(e => e.Email)</td>
            <td>@Html.ValidationMessageFor(e => e.Email)</td>

        </tr>
        <tr>
            <td>password</td>
            <td>@Html.PasswordFor(e => e.password)</td>
            <td>@Html.ValidationMessageFor(e => e.password)</td>


        </tr>
        <tr>
            <td>@Html.LabelFor(e => e.confirm_password)</td>
            <td>@Html.PasswordFor(e => e.confirm_password)</td>
            <td>@Html.ValidationMessageFor(e => e.confirm_password)</td>

        </tr>
        <tr>
            <td>Department</td>
            <td>@Html.DropDownListFor(e => e.departmentid, (SelectList)ViewBag.st)</td>

        </tr>
        <tr>
            <td>image</td>
            <td><input type="file" name="photo" /></td>
            <td>@Html.ValidationMessageFor(e => e.img)</td>
        </tr>
    </table>
    <br />
    <br />
    <button type="submit">Register</button>
</form>